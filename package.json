{
  "name": "mobile-web-fe-neeu",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start:dev": "npx yarn env:dev && npx expo start --clear",
    "start:staging": "npx yarn env:staging && npx expo start -c",
    "start:prod": "npx yarn env:prod && npx expo start -c",
    "android:dev": "APP_VARIANT=development npx expo run:android",
    "android:staging": "APP_VARIANT=staging npx expo run:android",
    "android:prod": "APP_VARIANT=production npx expo run:android",
    "ios:dev": "APP_VARIANT=development npx expo run:ios",
    "ios:staging": "APP_VARIANT=staging npx expo run:ios",
    "ios:prod": "APP_VARIANT=production npx expo run:ios",
    "web:dev": "APP_VARIANT=development npx expo start --web",
    "web:staging": "APP_VARIANT=staging npx expo start --web",
    "web:prod": "APP_VARIANT=production npx expo start --web",
    "prebuild:dev": "APP_VARIANT=development npx expo prebuild --clean",
    "prebuild:staging": "APP_VARIANT=staging npx expo prebuild --clean",
    "prebuild:prod": "APP_VARIANT=production npx expo prebuild --clean",
    "env:dev": "ts-node ./scripts/set-env development",
    "env:staging": "ts-node ./scripts/set-env staging",
    "env:prod": "ts-node ./scripts/set-env production",
    "eas:config": "ts-node ./scripts/set-eas",
    "build-android:dev-debug": "npx yarn env:dev && npx yarn eas:config && npx eas build --platform android --profile development-debug",
    "build-ios:dev-debug": "npx yarn env:dev && npx yarn eas:config && npx eas build --platform ios --profile development-debug",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "expo": "~50.0.11",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.9",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-status-bar": "~1.11.1",
    "jotai": "^2.7.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-mmkv": "^2.12.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.34.0",
    "postcss": "^8.4.35",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "*"
  },
  "private": true
}
